{
  "manifest_version": 2,

  "name": "Progress Tracker Extension",
  "description": "This extension will allow A/a students to easily enter application on Progress Tracker",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "http://progress.appacademy.io/me/jobberwocky/job_applications",
    "activeTab",
    "tabs"
  ]
}
